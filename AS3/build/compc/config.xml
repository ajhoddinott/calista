<?xml version="1.0" encoding="utf-8"?>

<flex-config>
    <compiler>
        <source-path>
            <path-element>../../src/</path-element>
        </source-path>
        
        <namespaces>
            <namespace>
                <uri>http://code.google.com/p/calista/2010</uri>
                <manifest>./manifests/calista_manifest.xml</manifest>
            </namespace>
        </namespaces>
        
        <strict>true</strict>
        <optimize>true</optimize>
    </compiler>
    
    <include-namespaces>
        <uri>http://code.google.com/p/calista/2010</uri>
    </include-namespaces>
    
    <target-player>10.0.0</target-player>
    
    <directory>false</directory>
    
    <output>../../libs/calista.swc</output>
    
    <metadata>
        <title>The AS3 Calista framework - version 1.0.0.25</title>
        <description>All class of the CALISTA library</description>
        <publisher>http://www.ekameleon.net</publisher>
        <creator>Marc ALCARAZ (aka eKameleon)</creator>
        <language>EN</language>
    </metadata>

</flex-config>
